{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "moonscript",
      "camelcase": "MoonScript",
      "title": "MoonScript",
      "scope": "source.moonscript",
      "file-types": [
        "moon"
      ],
      "injection-regex": "^moonscript$",
      "class-name": "TreeSitterMoonscript"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Moonscript grammar for tree-sitter",
    "authors": [
      {
        "name": "ABuffSeagull",
        "email": "reecevanatta@hey.com",
        "url": "https://abuffseagull.dev/"
      }
    ],
    "links": {
      "repository": "https://github.com/ABuffSeagull/tree-sitter-moonscript"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}